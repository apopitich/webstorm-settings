    <extensionMap>
      <mapping pattern=".gitkeep" type="PLAIN_TEXT" />
      <mapping ext="php" type="PLAIN_TEXT" />
      <mapping ext="py" type="PLAIN_TEXT" />
      <mapping pattern="reload-or-pay" type="XHTML" />
    </extensionMap>
  </component>
</application>